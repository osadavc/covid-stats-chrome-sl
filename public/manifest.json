{
  "manifest_version": 2,
  "name": "Covid Meter SL",
  "description": "This will show Sri Lanka's covid cases count with comparisons based on previous day",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Covid Meter SL"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "icons": {
    "16": "covid16.png",
    "48": "covid48.png",
    "128": "covid128.png"
  },
  "permissions": ["activeTab", "tabs", "<all_urls>"]
}
